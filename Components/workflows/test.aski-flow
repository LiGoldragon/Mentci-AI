;; Mentci-AI: Aski-Flow Test Workflow
;; Implements a codergen loop with human review.

[ (ImplementationPlan
    {:type :codergen
     :prompt "Write a robust hello world in Rust with error handling."})
  {:success Review}

  (Review
    {:type :wait.human})
  {:pass :next
   :fail ImplementationPlan}

  (DeliveredArtifact
    {:type :result
     :label "Great Work Complete"})
]
